  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Mocha Current",
      "program": "${workspaceFolder}/node_modules/mocha/bin/_mocha",
      "args": ["--timeout", "999999", "${file}"],
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "mocha-play Current",
      "program": "${workspaceFolder}/node_modules/mocha-play/bin/mocha-play.js",
      "args": ["${file}", "-w"],
      "outputCapture": "std",
      "internalConsoleOptions": "openOnSessionStart"
    },
    {
      "type": "node",
      "request": "attach",
      "name": "Attach",
      "port": 9229
    }
  ]
}
