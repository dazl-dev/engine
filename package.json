{
  "name": "engine",
  "workspaces": [
    "examples/*",
    "packages/*",
    "test-fixtures/*",
    "test-fixtures/workspace/packages/*"
  ],
  "scripts": {
    "clean": "rimraf \"./packages/*/dist\" \"./examples/*/dist\" \"./test-fixtures/*/dist\" \"./test-fixtures/workspace/packages/*/dist\"",
    "build": "yarn build:typescript && yarn build:stylable",
    "build:typescript": "tsc --build",
    "build:stylable": "lerna run build:stylable",
    "lint": "eslint .",
    "prestart": "yarn build",
    "start": "engineer start",
    "pretest": "yarn lint && yarn build",
    "test": "yarn test:unit",
    "test:unit": "lerna run test --stream --concurrency=1",
    "prettify": "npx prettier . --write"
  },
  "devDependencies": {
    "@file-services/memory": "^7.0.1",
    "@file-services/types": "^7.0.1",
    "@stylable/cli": "^5.3.0",
    "@stylable/webpack-plugin": "^5.3.0",
    "@ts-tools/node": "^4.0.0",
    "@ts-tools/webpack-loader": "^4.0.0",
    "@types/chai": "^4.3.4",
    "@types/chai-as-promised": "^7.1.5",
    "@types/cors": "^2.8.12",
    "@types/d3-hierarchy": "^3.1.0",
    "@types/d3-selection": "^3.0.3",
    "@types/d3-shape": "^3.1.0",
    "@types/express": "^4.17.14",
    "@types/html-minifier-terser": "^7.0.0",
    "@types/is-ci": "^3.0.0",
    "@types/minimist": "^1.2.2",
    "@types/mocha": "^10.0.0",
    "@types/node": "16",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@types/rimraf": "^3.0.2",
    "@types/semver": "^7.3.13",
    "@types/sinon": "^10.0.13",
    "@types/sinon-chai": "^3.2.9",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "@wixc3/common": "^2.2.1",
    "@wixc3/create-disposables": "^2.2.0",
    "@wixc3/wait-for-call": "^3.0.3",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "electron": "^21.2.3",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.5.0",
    "lerna": "^6.0.3",
    "mocha": "^10.1.0",
    "mocha-play": "^3.1.0",
    "playwright-chromium": "^1.27.1",
    "playwright-core": "^1.27.1",
    "promise-assist": "^1.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
    "sinon": "^14.0.2",
    "sinon-chai": "^3.7.0",
    "source-map-loader": "^4.0.1",
    "typescript": "~4.9.3",
    "util": "^0.12.5",
    "webpack": "^5.75.0"
  },
  "license": "MIT",
