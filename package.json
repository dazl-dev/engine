{
  "name": "engine",
  "workspaces": [
    "examples/*",
    "packages/*",
    "test-fixtures/*",
    "test-fixtures/workspace/packages/*"
  ],
  "scripts": {
    "clean": "rimraf -g \"./packages/*/dist\" \"./examples/*/dist\" \"./test-fixtures/*/dist\" \"./test-fixtures/workspace/packages/*/dist\"",
    "build": "yarn build:typescript && yarn build:stylable",
    "build:typescript": "tsc --build",
    "build:stylable": "lerna run build:stylable",
    "lint": "eslint .",
    "prestart": "yarn build",
    "start": "engineer start",
    "pretest": "yarn lint && yarn build",
    "test": "yarn test:unit",
    "test:unit": "lerna run test --stream --concurrency=1",
    "prettify": "npx prettier . --write"
  },
  "devDependencies": {
    "@file-services/memory": "^7.4.0",
    "@file-services/types": "^7.4.0",
    "@stylable/cli": "^5.15.0",
    "@stylable/webpack-plugin": "^5.15.0",
    "@ts-tools/webpack-loader": "^4.0.0",
    "@types/chai": "^4.3.5",
    "@types/chai-as-promised": "^7.1.5",
    "@types/cors": "^2.8.13",
    "@types/d3-hierarchy": "^3.1.2",
    "@types/d3-selection": "^3.0.5",
    "@types/d3-shape": "^3.1.1",
    "@types/debug": "^4.1.8",
    "@types/express": "^4.17.17",
    "@types/fs-extra": "^11.0.1",
    "@types/html-minifier-terser": "^7.0.0",
    "@types/is-ci": "^3.0.0",
    "@types/minimist": "^1.2.2",
    "@types/mocha": "^10.0.1",
    "@types/node": "18",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/semver": "^7.5.0",
    "@types/sinon": "^10.0.15",
    "@types/sinon-chai": "^3.2.9",
    "@types/yargs": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "@wixc3/create-disposables": "^2.2.0",
    "@wixc3/docs": "^12.0.4",
    "@wixc3/testing": "^12.0.3",
    "@wixc3/testing-node": "^12.0.3",
    "@wixc3/wait-for-call": "^12.0.1",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "electron": "^25.3.0",
    "electron-builder": "^24.4.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.5.3",
    "lerna": "^7.1.4",
    "mocha": "^10.2.0",
    "mocha-play": "^6.0.0",
    "playwright-chromium": "^1.36.1",
    "playwright-core": "^1.36.1",
    "promise-assist": "^2.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.1",
