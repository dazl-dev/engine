{
  "name": "engine",
  "workspaces": [
    "examples/*",
    "packages/*",
    "test-fixtures/*",
    "test-fixtures/workspace/packages/*"
  ],
  "scripts": {
    "clean": "rimraf \"./packages/*/dist\" \"./examples/*/dist\" \"./test-fixtures/*/dist\" \"./test-fixtures/workspace/packages/*/dist\"",
    "build": "yarn build:typescript && yarn build:stylable",
    "build:typescript": "tsc --build",
    "build:stylable": "lerna run build:stylable",
    "lint": "eslint .",
    "prestart": "yarn build",
    "start": "engineer start",
    "pretest": "yarn lint && yarn build",
    "test": "yarn test:unit",
    "test:unit": "lerna run test --stream --concurrency=1",
    "prettify": "npx prettier . --write"
  },
  "devDependencies": {
    "@file-services/memory": "^5.7.1",
    "@file-services/types": "^5.7.1",
    "@stylable/cli": "^4.9.3",
    "@stylable/core": "^4.9.3",
    "@stylable/webpack-plugin": "^4.9.3",
    "@ts-tools/build": "^2.1.1",
    "@ts-tools/node": "^3.0.1",
    "@ts-tools/webpack-loader": "^3.0.2",
    "@types/chai": "^4.3.0",
    "@types/chai-as-promised": "^7.1.4",
    "@types/cors": "^2.8.12",
    "@types/d3-hierarchy": "^3.0.2",
    "@types/d3-selection": "^3.0.2",
    "@types/d3-shape": "^3.0.2",
    "@types/express": "^4.17.13",
    "@types/html-minifier-terser": "^6.1.0",
    "@types/is-ci": "^3.0.0",
    "@types/minimist": "^1.2.2",
    "@types/mocha": "^9.0.0",
    "@types/node": "16",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/rimraf": "^3.0.2",
    "@types/semver": "^7.3.9",
    "@types/sinon": "^10.0.6",
    "@types/sinon-chai": "^3.2.7",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "@wixc3/create-disposables": "^1.0.0",
    "@wixc3/wait-for-call": "^1.0.0",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-no-only-tests": "^2.6.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "html-webpack-plugin": "^5.5.0",
    "lerna": "^4.0.0",
    "mocha": "^9.1.3",
    "mocha-play": "^3.0.1",
    "playwright-chromium": "^1.17.1",
    "playwright-core": "^1.17.1",
    "promise-assist": "^1.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "sinon": "^12.0.1",
    "sinon-chai": "^3.7.0",
    "source-map-loader": "^3.0.0",
    "typescript": "~4.5.4",
    "util": "^0.12.4",
    "webpack": "^5.65.0"
  },
  "license": "MIT",
  "private": true
}
