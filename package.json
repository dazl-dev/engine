{
  "name": "engine",
  "private": true,
  "type": "module",
  "workspaces": [
    "examples/*",
    "packages/*",
    "test-fixtures/*",
    "test-fixtures/workspace/packages/*"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf -g \"./packages/*/dist\" \"./examples/*/dist\" \"./test-fixtures/*/dist\" \"./test-fixtures/workspace/packages/*/dist\"",
    "build": "npm run build:typescript && npm run build:dashboard",
    "build:typescript": "tsc --build",
    "build:dashboard": "node build-dashboard.js",
    "lint": "eslint",
    "prestart": "npm run build",
    "start": "engine --dev",
    "pretest": "npm run lint && npm run build",
    "test": "npm run test:unit",
    "test:unit": "npm test --workspaces --if-present",
    "prettify": "prettier . --write"
  },
  "devDependencies": {
    "@dazl/create-disposables": "^18.6.0",
    "@file-services/memory": "^11.0.1",
    "@file-services/types": "^11.0.1",
    "@playwright/browser-chromium": "^1.57.0",
    "@types/chai": "^5.2.3",
    "@types/chai-as-promised": "^8.0.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/minimist": "^1.2.5",
    "@types/mocha": "^10.0.10",
    "@types/node": "22",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/sinon": "^21.0.0",
    "@types/sinon-chai": "^4.0.0",
    "@types/yargs": "^17.0.35",
    "@dazl/wait-for-call": "^18.6.1",
    "chai": "^6.2.1",
    "chai-as-promised": "^8.0.2",
    "create-temp-directory": "^2.4.0",
    "esbuild": "^0.27.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-no-only-tests": "^3.3.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "mocha": "^11.7.5",
    "mocha-web": "^2.5.1",
    "prettier": "^3.7.3",
    "promise-assist": "^2.0.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "rimraf": "^6.1.2",
    "sinon": "^21.0.0",
    "sinon-chai": "^4.0.1",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.48.1",
    "typescript-type-utils": "^0.1.0"
  },
  "license": "MIT"
}
