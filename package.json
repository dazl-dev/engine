{
  "name": "engine",
  "workspaces": [
    "examples/*",
    "packages/*",
    "test-fixtures/*",
    "test-fixtures/workspace/packages/*",
    "guides"
  ],
  "scripts": {
    "clean": "rimraf -g \"./packages/*/dist\" \"./examples/*/dist\" \"./test-fixtures/*/dist\" \"./test-fixtures/workspace/packages/*/dist\"",
    "build": "yarn build:typescript && yarn build:stylable",
    "build:typescript": "tsc --build",
    "build:stylable": "lerna run build:stylable",
    "lint": "eslint .",
    "prestart": "yarn build",
    "start": "engineer start",
    "pretest": "yarn lint && yarn build",
    "test": "yarn test:unit",
    "test:unit": "lerna run test --stream --concurrency=1",
    "prettify": "npx prettier . --write"
  },
  "devDependencies": {
    "@file-services/memory": "^8.3.2",
    "@file-services/types": "^8.3.2",
    "@playwright/browser-chromium": "^1.39.0",
    "@stylable/cli": "^5.16.0",
    "@stylable/webpack-plugin": "^5.16.0",
    "@types/chai": "^4.3.9",
    "@types/chai-as-promised": "^7.1.7",
    "@types/cors": "^2.8.15",
    "@types/d3-hierarchy": "^3.1.5",
    "@types/d3-selection": "^3.0.8",
    "@types/d3-shape": "^3.1.4",
    "@types/debug": "^4.1.10",
    "@types/express": "^4.17.20",
    "@types/fs-extra": "^11.0.3",
    "@types/html-minifier-terser": "^7.0.1",
    "@types/is-ci": "^3.0.3",
    "@types/minimist": "^1.2.4",
    "@types/mocha": "^10.0.3",
    "@types/node": "18",
    "@types/react": "^18.2.34",
    "@types/react-dom": "^18.2.14",
    "@types/semver": "^7.5.4",
    "@types/sinon": "^10.0.20",
    "@types/sinon-chai": "^3.2.11",
    "@types/yargs": "^17.0.29",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "@wixc3/create-disposables": "^2.2.0",
    "@wixc3/docs": "^13.3.2",
    "@wixc3/testing": "^13.3.2",
    "@wixc3/testing-node": "^13.3.2",
    "@wixc3/wait-for-call": "^13.3.2",
    "chai": "^4.3.10",
    "chai-as-promised": "^7.1.1",
    "electron": "^27.0.3",
    "electron-builder": "^24.6.4",
    "esbuild": "^0.19.4",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.5.3",
    "lerna": "^7.4.2",
    "mocha": "^10.2.0",
    "mocha-play": "^7.0.1",
    "playwright-core": "^1.39.0",
    "prettier": "^3.0.3",
